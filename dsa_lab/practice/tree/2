#include "tree.h"

struct stack {
	int top;
	nodeptr arr[50];
} st;

void push(nodeptr n) {
	if (st.top == 50) printf("Overflow!!\n");
	st.arr[++st.top] = n;
}

nodeptr pop() {
	if (st.top == -1) printf("Underflow!\n");
	return st.arr[st.top--];
}

void initStack() {
	st.top = -1;
}

void inorderI(nodeptr root) {
	nodeptr temp = root;
	int count = 0;
	while (count++ < 10) {
		if (temp->lst != NULL) {
			push(temp);
			temp = temp->lst;
			continue;
		}
		else {
			printf("%d ", temp->data);
		}
		if (temp->rst != NULL) {
			push(temp);
			temp = temp->rst;
			continue;
		}
		else {
			temp = pop();
		}
	}
}
